- if user_signed_in?
  %li= link_to 'Edit account', edit_user_registration_path
  %li= link_to 'Sign out', destroy_user_session_path, :method=>'delete'
- else
  /%li= link_to 'Sign in', new_user_session_path
  /%li= link_to 'Sign up user', new_user_registration_path(query: "user")
  /%li= link_to 'Sign up family member', new_user_registration_path(query: "family")
  /%li= link_to 'Sign up service provider', new_user_registration_path(query: "serviceprovider")